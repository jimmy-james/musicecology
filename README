pip install Django==1.3.1 PIL==1.1.7 django-filebrowser==3.3.0 django-grappelli==2.3.5 python-dateutil==1.5 vobject==0.8.1c -e hg+http://bitbucket.org/ubernostrum/django-contact-form/#egg=django-contact-form

add following to lib/python2.7/site-packages/grappelli/templates/admin/base.html
<script src="/static/grappelli/tinymce/jscripts/tiny_mce/tiny_mce.js" type="text/javascript"></script>
<script src="/static/grappelli/tinymce_setup/tinymce_setup.js" type="text/javascript"></script>

git clone git://github.com/jpells/django-agenda.git
cd django-agenda/
python setup.py install


WEBFACTION
pip-2.7 install --install-option="--install-scripts=$PWD/bin" --install-option="--install-lib=$PWD/lib/python2.7" PIL==1.1.7 django-filebrowser==3.3.0 django-grappelli==2.3.5 python-dateutil==1.5 vobject==0.8.1c -e hg+http://bitbucket.org/ubernostrum/django-contact-form/#egg=django-contact-form
